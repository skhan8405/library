{
    "name": "@neo-ui/filter",
    "version": "1.0.0",
    "description": "A library for iCargo component",
    "author": "iCargopoc",
    "license": "MIT",
    "repository": "iCargopoc/filter",
    "main": "dist/index.js",
    "module": "dist/index.modern.js",
    "source": "src/index.js",
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "build": "SET NODE_ENV=dev && microbundle-crl --no-compress --format modern,cjs",
        "start": "SET NODE_ENV=dev && microbundle-crl watch --no-compress --format modern,cjs",
        "prepare": "run-s build",
        "test": "jest --coverage --env=jest-environment-jsdom-sixteen",
        "test:build": "run-s build",
        "lint": "eslint src/**/*.js src/*.js",
        "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
        "test:watch": "react-scripts test --env=jsdom",
        "predeploy": "cd example && npm install && npm run build",
        "deploy": "gh-pages -d example/build",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook"
    },
    "peerDependencies": {
        "react": "^16.11.0",
        "react-dom": "^16.11.0",
        "multiselect-react-dropdown": "^1.5.7",
        "react-bootstrap": "^1.0.1",
